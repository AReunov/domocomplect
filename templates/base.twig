{% block html_head_container %}

{% include 'html-header.twig' %}
	{% block head %}
	{% endblock %}
	</head>
{% endblock %}

	<body class="{{body_class}}" data-template="base.twig">
	<div class="wrapper">
		<header class="header">
			<div class="container header__container">
				{{ fn('the_custom_logo') }}
				<div class="header__navigation">
					<div class="header__navigation__contacts">
						<div class="header__navigation__facebook">
							<a href="{{ header_facebook_link }}" target="_blank" id="header_facebook_link">
								<i class="icofont-facebook"></i>
							</a>
						</div>
						<div class="header__navigation__phone" id="header_phone_link">
							<a href="tel://0800336165" class="header__navigation__phone__icon" id="header__navigation__phone">
								<i class="icofont-phone"></i>
							</a>
							<a href="tel://0800336165" class="header__navigation__phone__text">
								0 <span>(800)</span> 336-165<br>
								Бесплатно <span>по</span> Украине
							</a>
						</div>
						<button class="toggle-menu-mobile">
							<i class="icofont-navigation-menu"></i>
						</button>
					</div>
					<nav class="main-navigation">
						<div class="header__navigation__menu">
							{% include "menu.twig" with {'items': menu.get_items} %}
						</div>


					</nav>
				</div>
